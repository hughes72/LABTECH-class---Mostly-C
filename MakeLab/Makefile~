#Makefile for 2-part print
SOURCES = main.c part1.c part2.c
OBJECTS = main.o part1.o part2.o
PROGRAMS = test2
$(PROGRAMS) : $(OBJECTS)
	cc -o $@ -O $(OBJECTS)

.SUFFIXES:
.SUFFIXES: .c .o

.c.o: ; cc -c $<
clean: ; /bin/rm -f $(PROGRAM) $(OBJECTS) core
depend: $(OBJECTS)
	makedepend $(OBJECTS)
# DO NOT DELETE
