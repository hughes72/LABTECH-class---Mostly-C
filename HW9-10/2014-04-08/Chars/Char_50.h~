    0x00,     // ........
    0x3c,     // .XXXX...
    0x44,     // .X...X..
    0x44,     // .X...X..
    0x3c,     // .XXXX...
    0x40,     // .X......
    0x40,     // .X......
    0x40,     // .X......
    0x00,     // ........
    0x00      // ........
};

